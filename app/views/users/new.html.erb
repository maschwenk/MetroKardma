<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.label :picture %>
  <%= f.text_field :picture %><br>

  <%= f.submit :Register %>
<% end %>