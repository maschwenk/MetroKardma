<!DOCTYPE html>
<html>
<head>
  <title>MetroKardma</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if logged_in? %>
  <%= link_to "log out", logout_path, method: :delete %>
<% else %>
  <%= link_to "Register", register_path %>
  <%= link_to "Log In", login_path %>
<% end %>

<%= yield %>

</body>
</html>
